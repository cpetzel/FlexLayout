  "buttons": {
    "primaryCTA": {
      "width": "82%",
      "height": "50",
      "x": "50% - (width/2)",
      "y": "{secondaryCTA.y} - height - 16",
      "cornerRadius": "25",
      "states": {
        "normal": {
          "text": "SIGN UP",
          "size": "16",
          "textColor": "#FFFFFF",
          "font": "Avenir-Black",
          "style": "normal",
          "alignment": "center",
          "backgroundColor": "#00000000",
          "backgroundGradient": {
            "type": "gradient",
            "baseColor": "#00000000",
            "colors": [
              "#22b6e7ff",
              "#22b6e7ff"
            ],
            "style": "vertical"
          },
          "borderSize": "0",
          "borderColor": "#25305e00",
          "alpha": "1",
          "image": "",
          "shadow": [
            "#00000000",
            2,
            0,
            0,
            0
          ]
        },
        "pressed": {
          "backgroundColor": "#00000000",
          "backgroundGradient": {
            "type": "gradient",
            "baseColor": "#00000000",
            "colors": [
              "#1ea3cfff",
              "#1ea3cfff"
            ],
            "style": "vertical"
          },
          "borderSize": "0",
          "borderColor": "#25305e00",
          "alpha": "1"
        }
      },
      "onClick": {
        "params": {
          "closeSelf": "true",
          "url": "kittyclothes://products/parka",
          "flowName": "SIGNUP"
        },
        "func": "startFlow"
      }
    },
    "secondaryCTA": {
      "width": "82%",
      "height": "50",
      "x": "50% - (width/2)",
      "y": "100% - height - 16",
      "cornerRadius": "25",
      "states": {
        "normal": {
          "text": "Connect with Facebook",
          "size": "14",
          "textColor": "#d3d9dc",
          "font": "Avenir-Light",
          "style": "normal",
          "alignment": "center",
          "backgroundColor": "#00000000",
          "backgroundGradient": {
            "colors": [
              "#ffffff00",
              "#ffffff00"
            ],
            "baseColor": "#00000000",
            "type": "gradient",
            "style": "vertical"
          },
          "borderSize": "2",
          "borderColor": "#898f95",
          "alpha": "1",
          "shadow": [
            "#00000000",
            1,
            1,
            1,
            0
          ],
          "image": "http://cdn.goprimer.com/icons/pmr04404f3f42d7b-3d8e-4ba2-9015-e007140bf689-facebook3.png",
          "leftImage": "http://cdn.goprimer.com/icons/pmr04404f3f42d7b-3d8e-4ba2-9015-e007140bf689-facebook3.png",
          "contentMode": "left"
        },
        "pressed": {
          "borderSize": "1",
          "alpha": "1",
          "backgroundGradient": {
            "colors": [
              "#ffffff00",
              "#ffffff00"
            ],
            "baseColor": "#00000000",
            "type": "gradient",
            "style": "vertical"
          }
        }
      },
      "onClick": {
        "params": {
          "successAction": {
            "params": {
              "closeSelf": "true"
            },
            "func": "close"
          },
          "closeSelf": "false"
        },
        "func": "loginFacebook"
      }
    }
  },
  "shapes": {
    "backgroundFilter": {
      "width": "100%",
      "height": "100%",
      "x": "0",
      "y": "0",
      "color": "#00000000",
      "backgroundGradient": {
        "colors": [
          "#0000007f",
          "#0000007f"
        ],
        "baseColor": "#00000000",
        "type": "gradient",
        "style": "vertical"
      },
      "borderSize": "0",
      "borderColor": "#000000",
      "cornerRadius": "0",
      "alpha": "1"
