      "contentMode": "aspectFit",
      "borderSize": "0",
      "borderColor": "#000000",
      "cornerRadius": "0",
      "alpha": "1",
      "required": "true",
      "backgroundGradient": {
        "colors": [
          "#00000000",
          "#00000000"
