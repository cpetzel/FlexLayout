      "color": "#00000000",
      "backgroundGradient": {
        "colors": [
          "#f2724fff",
          "#f2724fff"
        ],
        "baseColor": "#00000000",
        "type": "gradient",
        "style": "vertical"
      },
      "borderSize": "1",
      "borderColor": "#ef553b",
      "cornerRadius": "circle",
      "alpha": "1"
    }
