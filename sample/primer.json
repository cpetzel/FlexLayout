    "textCard": {
      "zLayer": "700",
      "width": "82%",
      "height": "width*.7",
      "x": "50% - (width/2)",
      "y": "{logo.y} + {logo.height} + 18%",
      "color": "#00000000",
      "backgroundGradient": {
        "colors": [
          "#ffffffff",
          "#ffffffff"
        ],
        "baseColor": "#00000000",
        "type": "gradient",
        "style": "vertical"
      },
      "borderSize": "0",
      "borderColor": "#f2724f",
      "cornerRadius": "3",
      "alpha": "1"
