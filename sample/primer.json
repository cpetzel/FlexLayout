      "borderColor": "#ffffff",
      "cornerRadius": "52",
      "alpha": "1",
      "required": "true",
      "backgroundGradient": {
        "colors": [
          "#ffffff00",
          "#ffffff00"
        ],
        "baseColor": "#00000000",
        "type": "gradient",
        "style": "vertical"
      },
      "backupImage": "",
      "tempImage": ""
